<?xml version="1.0" encoding="UTF-8" ?>
<WidgetButton>
	<Name>ButtonSettings</Name>
	<Priority>250</Priority>
	<Placement>
		<X>
			 <Align>WIDGET_ALIGN_CENTER</Align>
			<Size>40</Size>
		</X>
		<Y>
			<Align>WIDGET_ALIGN_CENTER</Align>
			<Size>40</Size>
		</Y>
	</Placement>
	<Enabled>true</Enabled>
	<IgnoreDblClick>true</IgnoreDblClick>
